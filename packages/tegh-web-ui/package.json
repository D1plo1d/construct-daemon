{
  "name": "@tegh/web-ui",
  "version": "0.4.0",
  "main": "index.js",
  "files": [
    "dist/*"
  ],
  "author": "Rob Gilson",
  "license": "AGPL-3.0-or-later",
  "private": false,
  "scripts": {
    "serve": "mkdir -p ./dev-dist && cd ./dev-dist && static-server -p 1234",
    "serve:production": "mkdir -p ./dist && cd ./dist && static-server -p 1234",
    "serve:direct": "parcel serve ./src/index.html -d ./dev-dist",
    "watch": "parcel watch ./src/index.html -d ./dev-dist --hmr-hostname localhost",
    "watch:production": "parcel watch ./src/index.html --no-hmr",
    "build": "NODE_OPTIONS=\"--max-old-space-size=4096\" parcel build ./src/index.html ./src/http/index.html --detailed-report",
    "report": "xdg-open ./dist/report.html",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "dependencies": {
