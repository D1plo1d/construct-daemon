{
  "name": "tegh-server",
  "version": "0.4.0",
  "description": "graphql server for 3D printers",
  "scripts": {
    "test": "jest",
    "lint": "eslint src/",
    "flow": "flow",
    "clean": "rm -rf build && mkdir build",
    "build-deps": "cd ./node_modules/graphql-live-subscriptions/ && yarn build",
    "build": "yarn run clean && yarn run build-deps && babel -d ./build ./src -s",
    "watch": "yarn run clean && yarn run build-deps && babel -d ./build ./src -s --watch"
  },
  "main": "build/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/tegh/tegh-server.git"
  },
  "keywords": [
    "graphql"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-flow"
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/build/"
    ],
    "testEnvironment": "node"
  },
  "author": "Rob Gilson",
  "license": "GPL-3.0",
  "dependencies": {
    "apollo-engine": "^1.1.0",
    "apollo-server-koa": "^1.3.4",
    "babel-preset-flow": "^6.23.0",
    "bluebird": "^3.5.1",
    "dedent": "^0.7.0",
    "ejs": "^2.5.7",
    "graphql": "0.13.2",
    "graphql-live-subscriptions": "github:d1plo1d/graphql-live-subscriptions#feature/tree_diff",
    "graphql-scalars": "^0.1.5",
    "graphql-subscriptions": "^0.5.8",
    "graphql-type-json": "^0.2.0",
    "immutable": "^4.0.0-rc.9",
    "immutablediff": "^0.4.4",
    "immutablepatch": "^0.4.2",
    "iterall": "d1plo1d/iterall#a5ce300b3a7588c9b93a010fefeafe6f8c9eb89b",
    "jiff": "^0.7.3",
    "js-yaml": "^3.10.0",
    "koa": "^2.3.0",
    "koa-bodyparser": "^4.2.0",
    "koa-cors": "^0.0.16",
    "koa-router": "^7.2.1",
    "lodash": "^4.17.10",
    "raven": "^2.3.0",
    "raven-for-redux": "^1.2.0",
    "redux": "^3.5.1",
    "redux-immutable": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-saga": "d1plo1d/redux-saga",
    "redux-thunk": "^2.2.0",
    "redux-watch": "^1.1.1",
    "reselect": "^3.0.1",
    "strip-newlines": "^1.0.1",
    "subscriptions-transport-ws": "d1plo1d/subscriptions-transport-ws",
    "tmp-promise": "^1.0.4",
    "typiql": "^0.3.0",
    "untildify": "^3.0.2",
    "uuid": "^3.1.0",
    "vm2": "^3.5.2"
  },
  "resolutions": {
    "subscriptions-transport-ws/iterall": "d1plo1d/iterall#a5ce300b3a7588c9b93a010fefeafe6f8c9eb89b"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.49",
    "@babel/core": "^7.0.0-beta.49",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.49",
    "@babel/polyfill": "^7.0.0-beta.49",
    "@babel/preset-env": "^7.0.0-beta.49",
    "@babel/preset-flow": "^7.0.0-beta.49",
    "@babel/register": "^7.0.0-beta.49",
    "babel-core": "^7.0.0-beta.49",
    "babel-eslint": "^8.1.0",
    "babel-jest": "^23.0.1",
    "jest": "^23.1.0",
    "redux-saga-tester": "D1plo1d/redux-saga-tester"
  }
}
